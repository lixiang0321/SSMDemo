<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="springcache">
<diskStore path="java.io.tmpdir"/>
<defaultCache
            maxElementsInMemory="20000"
            eternal="true"
            timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            overflowToDisk="true"
/>
<cache name="student"
           maxElementsInMemory="2000"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="0"
           overflowToDisk="true"
/>

</ehcache>
